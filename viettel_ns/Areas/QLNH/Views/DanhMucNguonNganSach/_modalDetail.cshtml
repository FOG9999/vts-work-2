@using Viettel.Models.QLNH
@using System.Globalization;
@model DanhmucNgoaiHoi_NguonNganSachModel

<div class="modal-body">
    <div class="form-group row">
        <div class="col-sm-6">
            <label>Mã nguồn ngân sách</label>
            <p>@Html.Raw(Html.Encode(Model.iID_MaNguonNganSach))</p>
        </div>
        <div class="col-sm-6">
            <label>Tên nguồn ngân sách</label>
            <p>@Html.Raw(Html.Encode(Model.sTen))</p>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6">
            <label>Trạng thái</label>
            <p>@Html.Raw(Html.Encode(Model.sTrangThai))</p>
        </div>
        <div class="col-sm-6">
            <label>STT</label>
            <p>@Html.Raw(Html.Encode(Model.iSTT))</p>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>