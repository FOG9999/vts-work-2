﻿@model Viettel.Models.QLNH.QuyetToan.NH_QT_QuyetToanNienDo_ChiTietView
@{
    ViewBag.Title = "Báo cáo đề nghị tổng hợp số chuyển năm sau theo từng hợp đồng, dự án toàn quân";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}
@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}
<style>
    .m-w100 {
        min-width: 100px;
    }

    .f-wBold {
        font-weight: bold;
    }

    .mr-10 {
        margin-right: 10px;
    }
</style>
<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body" id="lstDataView">
                @Html.Partial("_baoCaoDetail", Model)
            </div>
        </div>
    </div>
</div>


@*Modal*@
<div class="modal fade modalBCTHSoChuyenNamSau" id="modalBCTHSoChuyenNamSau" tabindex="-1" role="dialog" aria-labelledby="modalBCTHSoChuyenNamSauLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="width:80%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalBCTHSoChuyenNamSauLabel"></h3>
            </div>
            <div id="contentModalBCTHSoChuyenNamSau"></div>
        </div>
    </div>
</div>

<div id="divModalConfirm">

</div>
@section Scripts{
    <script src="~/Scripts/QLNH/BaoCaoThongKe/jsBaoCaoTongHopSoChuyenNamSau.js"></script>

    <script>
            var sUrlListView = "";
            $(document).ready(function () {
                sUrlListView = '@Url.Action("TimKiem", "BaoCaoTongHopSoChuyenNamSau")';
                });
    </script>

}
