    <div class="modal-dialog" role="document" style="width:60%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalQTNienDoLabel"></h3>
            </div>
            <div id="contentModalQTNienDo">

            </div>
        </div>
    </div>
</div>

<div id="divModalConfirm">

</div>
@section Scripts{
    @Html.Partial("_SheetScript")
    <script src="~/Scripts/QLVonDauTu/QuyetToan/jsVdtQtBcQuyetToanNienDo.js"></script>
    <script>
        var _paging = {};

        var sUrlListView = "";
        $(document).ready(function () {
            _paging.CurrentPage = @Model._paging.CurrentPage ;
            _paging.ItemsPerPage = @Model._paging.ItemsPerPage ;
        });
    </script>
}

