
@{
    ViewBag.Title = "Quyết toán niên độ";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}

@using System.Data;
@using DomainModel;
@using DomainModel.Controls;
@using System.ComponentModel;
@using VIETTEL.Models;
@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}
<script src="@Url.Content("~/Scripts/jquery-1.4.4.min.js") " type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jsControl.js") "
        type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jsInit.js")"
        type="text/javascript"></script>
<script src="~/Scripts/jsMucLucNganSach.js"></script>

@{
    String MaND = User.Identity.Name;
    String IPSua = Request.UserHostAddress;
    String sLNS = Request.QueryString["sLNS"];
    String id = Request.QueryString["id"];
}


<div style="width: 100%; margin-top: 10px;">
    <div id="divChungTuChiTietHT" style="width: 100%; position: relative">
        <div class="custom_css box_tong">
            <div class="title_tong">
                <table cellpadding="0" cellspacing="0" border="0" width="100%">
                    <tr>
                        <td align="left">
                            <span>Chuyển quyết toán chi tiết</span>
                        </td>
                    </tr>
                </table>
            </div>
            <div>
                @Html.Partial("ChuyenDuLieuQuyetToan_Edit_Search")
                @*@Html.RenderPartial("~/Views/DungChung/MucLucNganSach/MucLucNganSach_Edit_Search.ascx", new { ControlID = "MucLucNganSach", MaND = User.Identity.Name, ChucNangCapNhap = true })*@
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        jsMLNS_LNS = "@sLNS";
        jsMLNS_Url_Frame = '@Url.Action("MLNSChiTiet_Frame", "ChuyenDuLieuQuyetToan" , new { id = id , sLNS= sLNS }  )';
        jsMLNS_Url = '@Url.Action("Edit", "ChuyenDuLieuQuyetToan", new { sLNS = sLNS})';
        $("#tabs").tabs();
    });
</script>



