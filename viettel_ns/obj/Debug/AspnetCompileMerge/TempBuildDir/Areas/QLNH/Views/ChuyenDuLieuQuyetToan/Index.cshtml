﻿@model Viettel.Models.QLNH.QuyetToan.ChuyenQuyetToan.QuyetToan_ChuyenQuyetToanModel
@{
    ViewBag.Title = "Chuyển dữ liệu quyết toán";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}
@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}
<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body" id="lstDataView">
                @Html.Partial("_list", Model)
            </div>
        </div>
    </div>
</div>

@*Modal*@
<div class="modal fade modalChuyenQuyetToan" id="modalChuyenQuyetToan" tabindex="-1" role="dialog" aria-labelledby="modalChuyenQuyetToanLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="width:80%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalChuyenQuyetToanLabel"></h3>
            </div>
            <div id="contentModalChuyenQuyetToan"></div>
        </div>
    </div>
</div>

<div id="divModalConfirm">

</div>
@section Scripts{
    <script src="~/Scripts/js/js_app.js"></script>
    <script src="~/Scripts/QLNH/ChuyenQuyetToan/jsChuyenQuyetToan.js"></script>
    <script>
        var _paging = {};
        var sUrlListView = "";
        $(document).ready(function () {
            sUrlListView = '@Url.Action("TimKiem", "ChuyenDuLieuQuyetToan")';
                _paging.CurrentPage = @Model._paging.CurrentPage ;
                _paging.ItemsPerPage = @Model._paging.ItemsPerPage ;
            });
    </script>

}

