@using Viettel.Models.QLVonDauTu
@model VDT_DM_ChuDauTu_ViewModel

<div class="modal-body">
    <div class="form-group">
        <div class="form-group row">
            <div class="col-sm-6">
                <label>Mã đơn vị</label>
                <p>@Html.Raw(Html.Encode(Model.sId_CDT))</p>
            </div>
            <div class="col-sm-6">
                <label>Tên đơn vị</label>
                <p>@Html.Raw(Html.Encode(Model.sTenCDT))</p>
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-6">
                <label>Ký hiệu</label>
                <p>@Html.Raw(Html.Encode(Model.sKyHieu))</p>
            </div>
            <div class="col-sm-6">
                <label>Mô tả</label>
                <p>@Html.Raw(!string.IsNullOrEmpty(Html.Encode(Model.sMoTa)) ? Html.Encode(Model.sMoTa).Replace("\n", "<br/>") : string.Empty)</p>
            </div>
        </div> <div class="form-group row">
            <div class="col-sm-6">
                <label>Loại</label>
                <p>@Html.Raw(Html.Encode(Model.sKyHieu))</p>
            </div>
            <div class="col-sm-6">
                <label>Chủ đầu tư cha</label>
                <p>@Html.Raw(Html.Encode(Model.sTenChuDauTuCha))</p>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <div class="text-center">
        <button class="btn btn-default" data-dismiss="modal"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
    </div>
</div>
