@using Viettel.Models.QLVonDauTu
@model KHChiQuyPagingModel

@{
    ViewBag.Title = "Danh sách kế hoạch nhu cầu chi quý";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}
<link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body" id="lstDataView" style="padding: 10px">
                @Html.Partial("_list", Model)
            </div>
        </div>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="modalKH5NamDeXuat" tabindex="-1" role="dialog" aria-labelledby="modalKH5NamDeXuatLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog" role="document" style="width:60%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalKH5NamDeXuatLabel"></h3>
            </div>
            <div id="contentModalKH5NamDeXuat">

            </div>
        </div>
    </div>
</div>

@* Modal confirm *@
<div id="divModalConfirm">

</div>

<!-- Modal -->
<div class="modal fade modalKeHoachChiQuy" id="modalKeHoachChiQuy" tabindex="-1" role="dialog" aria-labelledby="modalKeHoachChiQuyLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="width:60%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalKeHoachChiQuyLabel"></h3>
            </div>
            <div id="contentModalKeHoachChiQuy"></div>
        </div>
    </div>
</div>
<div id="divModalConfirm"></div>

@section Scripts{
    <script src="~/Scripts/QLVonDauTu/NganSachQuocPhong/jsVDT_DanhSachKHNhuCauChiQuy.js"></script>
    <script>
        var _paging = {};

        var sUrlListView = "";
        $(document).ready(function () {
            _paging.CurrentPage = @Model._paging.CurrentPage ;
            _paging.ItemsPerPage = @Model._paging.ItemsPerPage ;
        });
    </script>
}

