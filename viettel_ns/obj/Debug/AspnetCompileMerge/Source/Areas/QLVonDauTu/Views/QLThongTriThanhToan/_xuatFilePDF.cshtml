@{
    ViewBag.Title = "Xuất báo cáo";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}
<link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">

<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body create_thdt">
                <div class="form-group">
                    <h4 class="color-text-red">Xuất báo cáo:</h4>
                    <hr class='color-background-red' />
                    <input id="idInput" hidden value="@ViewBag.id" />
                    <div class="form-group">
                        <div class="form-group row">
                            <div class="col-sm-6">
                                <label for="sTieuDeMot">Tiêu đề 1</label>
                                <input type="text" id="sTieuDeMot" class="form-control requireInput" name="sTieuDeMot" value="" maxlength="50" autocomplete="off" />
                            </div>
                            <div class="col-sm-6">
                                <label for="sTieuDeHai">Tiêu đề 2</label>
                                <input type="text" id="sTieuDeHai" class="form-control requireInput" name="sTieuDeHai" value="" autocomplete="off" />
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-sm-6">
                                <label for="iDonViTinh">Đơn vị tính</label>
                                @Html.DropDownList("iDonViTinh", (SelectList)(ViewBag.lstDonViTinh), new { @class = "form-control requireInput", @id = "iDonViTinh" })
                            </div>
                        </div>
                    </div>
                    <div class="form-group text-center">
                        <button class="btn btn-default" style="width: 100%" onclick="Huy()"><i class="fa fa-ban" aria-hidden="true"></i><span>Hủy</span></button>
                        <button class="btn btn-primary" id="btnLuu" style="width: 100%" onclick="InBaoCao()"><i class="fa fa-download" aria-hidden="true"></i><span>In báo cáo</span></button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/QLVonDauTu/NganSachQuocPhong/jsVDT_XuatFileThongTriThanhToan.js" type="text/javascript"></script>
<script src="/Scripts/sheet/jsNumber.js" type="text/javascript"></script>
<script src="~/Scripts/js/js_vdtWeb.js" type="text/javascript"></script>
