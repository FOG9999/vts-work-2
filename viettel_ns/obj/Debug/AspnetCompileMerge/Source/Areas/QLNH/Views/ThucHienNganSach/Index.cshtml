@model Viettel.Models.CPNH.CPNHThucHienNganSach_ModelPaging
@{
    ViewBag.Title = "Tình hình thực hiện ngân sách";
    Layout = "~/Views/Shared/_LayoutWebNew.cshtml";
}


@section Styles{
    <link rel="stylesheet" type="text/css" href="~/Content/css/DanhMuc/style.css">
}

<div class="main">
    <div class="panel-list-item col-sm-12">
        <div class="card">
            <div class="card-body" id="lstDataView">
                @Html.Partial("_list", Model)
            </div>
        </div>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="modalThucHienNganSach" tabindex="-1" role="dialog" aria-labelledby="modalThucHienNganSach" aria-hidden="true">
    <div class="modal-dialog" role="document" style="width:60%">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title text-bold text-left" id="modalThucHienNganSachLabel"></h3>
            </div>
            <div id="contentModalThucHienNganSach">

            </div>
        </div>
    </div>
</div>
@* Modal confirm *@
<div id="divModalConfirm">

</div>

@section Scripts{
    <script src="~/Scripts/QLNH/ThucHienNganSach/jsThucHienNganSach.js"></script>
}